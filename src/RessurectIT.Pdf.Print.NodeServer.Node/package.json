{
    "name": "ressurectit.pdf.print.nodeserver",
    "version": "0.0.0-placeholder",
    "description": "NodeJS server that enables usage of pdf-to-printer using gRPC",
    "scripts": {
        "build": "tsc",
        "clean": "rimraf dist src/proto",
        "clean:deps": "rimraf package-lock.json node_modules",
        "clean:all": "npm run clean && npm run clean:deps",
        "tsc": "tsc",
        "generate:proto": "proto-loader-gen-types --grpcLib=@grpc/grpc-js --outDir=src/proto/ ../pdfPrint.proto",
        "lint": "eslint src/**/*.ts"
    },
    "engines": {
        "node": "^12.20.0 || ^14.15.0 || >=16.10.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ressurectit/RessurectIT.Pdf.Print.git"
    },
    "keywords": [
        "pdf",
        "print",
        "node",
        "grpc-server"
    ],
    "author": "kukjevov",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ressurectit/RessurectIT.Pdf.Print/issues"
    },
    "homepage": "https://github.com/ressurectit/RessurectIT.Pdf.Print",
    "dependencies": {
        "pdf-to-printer": "5.2.0",
        "portfinder": "1.0.28",
        "@grpc/proto-loader": "0.6.9",
        "@grpc/grpc-js": "1.5.3",
        "tslib": "2.3.1"
    },
    "devDependencies": {
        "@types/node": "^14.14.31",
        "@jscrpt/common": "^2.2.0",
        "eslint": "7.32.0",
        "@typescript-eslint/eslint-plugin": "4.33.0",
        "@typescript-eslint/parser": "4.33.0",
        "eslint-plugin-ressurectit": "0.1.0",
        "rimraf": "3.0.2",
        "typescript": "4.5.5"
    }
}
